    "numero = int(input(\"Ingrese un numero\"))\n",
    "lista = []\n",
    "\n",
    "\n",
    "while(numero != 0):\n",
    "    suma = 0\n",
    "    lista.append(numero)\n",
    "    for num in lista:\n",
    "        suma += num\n",
    "    numero = int(input(\"ingrese otro numero: \"))\n",
    "\n",
    "print(suma)\n",
    "\n",
    "\n",
    "\n"
   ]
